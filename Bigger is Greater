string biggerIsGreater(string w) {
    int i = w.length() - 2;

    while (i >= 0 && w[i] >= w[i + 1]) {
        i--;
    }

   
    if (i < 0) return "no answer";

    
    int j = w.length() - 1;
    while (w[j] <= w[i]) {
        j--;
    }

   
    swap(w[i], w[j]);

    reverse(w.begin() + i + 1, w.end());

    return w;


}
